    "react-select": "^5.8.0",
    "react-textarea-autosize": "^8.5.3",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "remark-squeeze-paragraphs": "^6.0.0",
    "remark-truncate-links": "^1.0.3",
    "rss-converter": "^1.1.6",
    "sonner": "^1.3.1",
    "stream": "^0.0.2",
    "string_decoder": "^1.3.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.4.1",
